    "lint:fix": "eslint \"src/**/*.{ts,js}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "serve:dev": "sls dynamodb install && sls offline start",
    "sort:packages": "npx sort-package-json",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --ci --verbose"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.16.1",
    "class-validator": "^0.12.2",
