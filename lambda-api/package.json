    "faker": "^4.1.0",
    "graphql": "^15.3.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0-rc.3",
    "typedi": "^0.8.0",
    "uuid": "^8.2.0",
    "webpack-node-externals": "^2.5.0",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
