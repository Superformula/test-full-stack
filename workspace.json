{
  "version": 1,
  "projects": {
    "user-profile-api": {
      "root": "apps/user-profile-api",
      "sourceRoot": "apps/user-profile-api/src",
      "projectType": "application",
      "prefix": "user-profile-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@flowaccount/nx-serverless:build",
          "options": {
            "outputPath": "dist/apps/user-profile-api",
            "package": "apps/user-profile-api",
            "serverlessConfig": "apps/user-profile-api/serverless.yml",
            "servicePath": "apps/user-profile-api",
            "tsConfig": "apps/user-profile-api/tsconfig.app.json",
            "provider": "aws",
            "processEnvironmentFile": "env.json"
          },
          "configurations": {
            "dev": {
              "optimization": false,
              "sourceMap": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "production": {
              "optimization": true,
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/user-profile-api/environment.ts",
                  "with": "apps/user-profile-api/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@flowaccount/nx-serverless:offline",
          "options": {
            "buildTarget": "user-profile-api:build",
            "config": "apps/user-profile-api/serverless.yml",
            "location": "dist/apps/user-profile-api"
          },
          "configurations": {
            "dev": {
              "buildTarget": "user-profile-api:build:dev"
            },
            "production": {
              "buildTarget": "user-profile-api:build:production"
            }
          }
        },
        "deploy": {
          "builder": "@flowaccount/nx-serverless:deploy",
          "options": {
            "buildTarget": "user-profile-api:build:production",
            "config": "apps/user-profile-api/serverless.yml",
            "location": "dist/apps/user-profile-api",
            "package": "dist/apps/user-profile-api"
          }
        },
        "destroy": {
          "builder": "@flowaccount/nx-serverless:destroy",
          "options": {
            "buildTarget": "user-profile-api:build:production",
            "config": "apps/user-profile-api/serverless.yml",
            "location": "dist/apps/user-profile-api",
            "package": "dist/apps/user-profile-api"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/user-profile-api/tsconfig.app.json", "apps/user-profile-api/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/user-profile-api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/user-profile-api/jest.config.js",
            "tsConfig": "apps/user-profile-api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "user-profile-api"
}
