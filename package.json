{
  "name": "Profiles",
  "version": "0.0.0",
  "engines": {
    "node": ">=6.6"
  },
  "description": "Profiles",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --hot --progress --color --port 3001",
    "build": "NODE_ENV=production webpack -p",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "yarn typecheck -- --watch",
    "test:watch": "npm test -- --watch",
    "schema:download": "yarn apollo client:download-schema --endpoint http://0.0.0.0:8000/graphql src/schema.graphqls",
    "generate": "yarn schema:download && apollo client:codegen --includes 'src/**/*.graphql' --localSchemaFile=src/schema.graphqls --target typescript --addTypename --globalTypesFile=src/types/__generated__/globalTypes.ts",
    "dev": " webpack-dashboard -- webpack-dev-server --progress --profile --colors",
    "lint": "yarn lint:typescript && yarn prettify --check",
    "format": "yarn lint:typescript --fix; yarn prettify --write",
    "lint:typescript": "eslint src --ext .js,.jsx,.ts,.tsx",
    "prettify": "prettier 'src/**/*.ts{,x}' 'src/**/*.graphql' 'src/**/*.scss' 'storybook/**/*.ts{,x}' 'webpack/**/*.js'"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "autoprefixer": "^9.6.0",
    "caniuse-api": "^1.6.1",
    "create-react-class": "^15.5.3",
    "graphql-tools": "^6.0.5",
    "js-cookie": "^2.1.4",
    "lodash": "^4.15.0",
    "minimist": "^1.2.5",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-icons": "2.2.7",
    "react-json-pretty": "^1.2.1",
    "react-json-tree": "^0.11.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-select": "^2.1.1",
    "rice-instructor": "^0.0.11",
    "set-value": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/graphql": "^14.5.0",
    "@types/node": "^8.0.26",
    "@types/pluralize": "0.0.27",
    "@types/query-string": "^5.0.0",
    "@types/react": "^16.9.19",
    "@types/react-router": "^5.0.1",
    "@types/webpack": "^4.4.4",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "apollo": "^2.28.0",
    "apollo-client": "^2.6.10",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "2.0.2",
    "cross-env": "^1.0.7",
    "enzyme": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.3",
    "mini-css-extract-plugin": "^0.7.0",
    "node-libs-browser": "^0.5.2",
    "prettier": "^2.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.12.21",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.9.5",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  }
}
